{
  "expo": {
    "name": "mai-aisha-academy",
    "slug": "mai-aisha-academy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/maa.jpg",
    "scheme": "maiaishaacademy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ibrac02.maiaishaacademy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/maa.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#87CEEB"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "33dc816e-6c3d-4fcd-a01c-81a4fed6e7c7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/33dc816e-6c3d-4fcd-a01c-81a4fed6e7c7"
    }
  }
}
